<phpunit bootstrap="./Bootstrap.php" colors="true">
    <testsuite name="Mutagenesis Test Suite">
        <directory>./</directory>
    </testsuite>

    <groups>
        <exclude>
            <group>PHPUnitRunnerTesting</group>
        </exclude>
    </groups>

    <filter>
        <whitelist>
            <directory suffix=".php">../library/</directory>
        </whitelist>
    </filter>
</phpunit>

